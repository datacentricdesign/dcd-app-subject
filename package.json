{
  "name": "dcd-app-subject",
  "version": "0.0.1-feature.designer-task",
  "repository": {
    "type": "git",
    "url": "https://github.com/datacentricdesign/dcd-data-subject"
  },
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --base-href /subject/",
    "build": "ng build --base-href /subject/",
    "build:prod": "ng build --prod --base-href /subject/",
    "build:client-and-server-bundles": "npm run build:prod && ng run dcd-data-subject:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "serve:ssr": "node dist/server",
    "prod": "npm run postinstall && npm run build:ssr && npm run serve:ssr",
    "watch:browser": "ng build --prod --base-href /subject/ --optimization=true --watch",
    "watch:browser://doesn't works hmr": "ng build --base-href /subject/ --watch",
    "predev": "ng run dcd-data-subject:server:production && npm run compile:server",
    "dev": "nodemon dist/server",
    "compile_@swimlane_ngx-charts": "babel node_modules/@swimlane/ngx-charts -d node_modules/@swimlane/ngx-charts --presets @babel/preset-env",
    "compile_angular-vertical-timeline": "babel node_modules/angular-vertical-timeline -d node_modules/angular-vertical-timeline --presets @babel/preset-env",
    "postinstall": "npm run compile_@swimlane_ngx-charts && npm run compile_angular-vertical-timeline"
  },
  "pre-commit": [],
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.0",
    "@angular/cdk": "8.0.2",
    "@angular/common": "8.1.3",
    "@angular/compiler": "8.1.3",
    "@angular/core": "8.1.3",
    "@angular/forms": "8.1.3",
    "@angular/http": "8.0.0-beta.10",
    "@angular/material": "8.0.2",
    "@angular/platform-browser": "8.1.3",
    "@angular/platform-browser-dynamic": "8.1.3",
    "@angular/platform-server": "8.1.3",
    "@angular/router": "8.1.3",
    "@datacentricdesign/sdk-js": "0.0.21-feature.designer-task",
    "@datacentricdesign/ui-angular": "0.1.3-dev",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "body-parser": "~1.19.0",
    "cookie-parser": "~1.4.4",
    "core-js": "^2.4.1",
    "cors": "~2.8.5",
    "domino": "^2.1.3",
    "dotenv": "^8.0.0",
    "express": "^4.15.2",
    "express-session": "~1.16.2",
    "find-config": "~1.0.0",
    "hammerjs": "~2.0.8",
    "nodemon": "^1.19.1",
    "passport": "~0.4.0",
    "passport-oauth2-refresh": "~1.1.0",
    "path": "~0.12.7",
    "reflect-metadata": "^0.1.10",
    "rxjs": "6.3.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.801.3",
    "@angular/cli": "8.1.3",
    "@angular/compiler-cli": "8.1.3",
    "@angular/language-service": "8.1.3",
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "~7.5.4",
    "@types/node": "^8.10.52",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "codelyzer": "^4.0.2",
    "http-server": "^0.10.0",
    "pre-commit": "^1.2.2",
    "ts-loader": "^4.2.0",
    "tslint": "^5.19.0",
    "typescript": "3.4.3"
  }
}
